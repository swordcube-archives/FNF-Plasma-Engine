<!DOCTYPE plasma-character >
<character antialiasing="false" icon="senpai" flip_x="false" >
    <animations>
        <!-- All offsets are the Week 7 ones divided by 6. -->
        <animation name="idle" anim="Senpai Idle" indices="" fps="24" looped="false" offsetX="0" offsetY="0" />

        <animation name="singLEFT" anim="SENPAI LEFT NOTE" indices="" fps="24" looped="false" offsetX="6" offsetY="0" />
        <animation name="singDOWN" anim="SENPAI DOWN NOTE" indices="" fps="24" looped="false" offsetX="2.3" offsetY="0" />
        <animation name="singUP" anim="SENPAI UP NOTE" indices="" fps="24" looped="false" offsetX="0.8" offsetY="6.1" />
        <animation name="singRIGHT" anim="SENPAI RIGHT NOTE" indices="" fps="24" looped="false" offsetX="0" offsetY="0" />
    </animations>

    <global_pos offsetX="150" offsetY="360" />
    <scale x="6" y="6" />
    <scroll x="1" y="1" />

    <color r="255" g="170" b="111" hex="" />

    <camera offsetX="-240" offsetY="-330" />
</character>